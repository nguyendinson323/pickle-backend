{
  "name": "pickleball-federation-backend",
  "version": "1.0.0",
  "description": "Backend API for Pickleball Federation Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "ts-node-dev --respawn --transpile-only --watch src src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:setup": "node scripts/setup-database.js",
    "db:reset:safe": "node scripts/reset-database.js",
    "db:seed:only": "node scripts/seed-only.js",
    "db:validate": "node scripts/validate-database.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.2",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sharp": "^0.32.5",
    "socket.io": "^4.8.1",
    "stripe": "^13.6.0",
    "twilio": "^4.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.5.0",
    "@types/qrcode": "^1.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "pickleball",
    "federation",
    "sports",
    "api",
    "backend",
    "typescript"
  ],
  "author": "Pickleball Federation",
  "license": "MIT"
}
